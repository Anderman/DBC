@model ApplicationUser
@using DBC.Models.DB
@{
  ViewBag.Title = "Edit User";
}
<div class="portlet light bordered">
    <div class="portlet-title">
        <div class="caption font-green-haze">
            <i class="fa-edit font-green-haze"></i>
            <span class="caption-subject bold uppercase">@ViewBag.Title</span>
        </div>
        <div class="actions">
            <a class="btn btn-circle btn-icon-only btn-default fullscreen" href="javascript:;" data-original-title="" title="">
            </a>
        </div>
    </div>
    <div class="portlet-body form">
        <form role="form" class="form-horizontal">
            <div class="form-body">
                <input asp-for="Id" asp-material-design="true" class="form-control" readonly />
                <input asp-for="UserName" asp-material-design="true" class="form-control" />
                <input asp-for="Email" asp-material-design="true" class="form-control" />
                <input asp-for="EmailConfirmed" asp-material-design="true" class="md-check" />
                <input asp-for="PhoneNumber" asp-material-design="true" class="form-control" />
                <input asp-for="PhoneNumberConfirmed" asp-material-design="true" class="form-control" />
                <input asp-for="TwoFactorEnabled" asp-material-design="true" class="form-control" />
                <input asp-for="LockoutEnabled" asp-material-design="true" class="md-check" />
                <input asp-for="LockoutEnd" asp-material-design="true" class="form-control" />
                <input asp-for="AccessFailedCount" asp-material-design="true" class="form-control" readonly />
                <input asp-for="Logins" asp-material-design="true" class="form-control" readonly />
                <input asp-for="Claims" asp-material-design="true" class="form-control" readonly />
                <input asp-for="Roles" asp-material-design="true" class="form-control" readonly />
                <input asp-for="SecurityStamp" asp-material-design="true" class="form-control" readonly />
                <input asp-for="NormalizedEmail" asp-material-design="true" class="form-control" readonly />
                <input asp-for="PasswordHash" asp-material-design="true" class="form-control" readonly />
            </div>
            <div class="form-actions">
                <div class="row">
                    <div class="col-md-offset-2 col-md-10">
                        <button type="button" class="btn default">Cancel</button>
                        <button type="button" class="btn blue">Submit</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>